{
  "name": "crewsplit",
  "version": "1.4.2",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "verify-migrations": "node scripts/verify-migrations.js",
    "arch-test": "dependency-cruiser --config .dependency-cruiser.js --output-type err src",
