    "@shopify/flash-list": "2.2.2",
    "@shopify/react-native-skia": "^2.4.21",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-preset-expo": "~54.0.10",
    "drizzle-orm": "^0.45.1",
